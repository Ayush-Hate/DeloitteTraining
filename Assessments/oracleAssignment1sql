<--1-->
SELECT *
FROM JOBS
WHERE MIN_SALARY > 10000;
<--2-->
SELECT FIRST_NAME, HIRE_DATE 
FROM EMPLOYEES
WHERE to_char(HIRE_DATE,'yy') BETWEEN 2 AND 5
ORDER BY HIRE_DATE;
<--3-->
SELECT e.FIRST_NAME, e.HIRE_DATE
FROM EMPLOYEES e, JOBS j 
WHERE e.JOB_ID = j.JOB_ID AND (j.JOB_TITLE = 'Sales Representative' OR j.JOB_TITLE = 'Programmer');
<--4-->
SELECT *
FROM EMPLOYEES 
WHERE HIRE_DATE > '01-JAN-08';
<--5-->
SELECT *
FROM EMPLOYEES
WHERE EMPLOYEE_ID = 150 OR EMPLOYEE_ID = 160;
<--6-->
SELECT FIRST_NAME, SALARY, COMMISSION_PCT, HIRE_DATE 
FROM EMPLOYEES
WHERE SALARY <10000;
<--7-->
SELECT JOB_TITLE, (MAX_SALARY - MIN_SALARY)
FROM JOBS
WHERE (MAX_SALARY - MIN_SALARY) BETWEEN 10000 AND 20000;
<--8-->
SELECT FIRST_NAME, ROUND(SALARY,-3)
FROM EMPLOYEES;
<--9-->
SELECT *
FROM JOBS
ORDER BY JOB_TITLE DESC;
<--10-->
SELECT *
FROM EMPLOYEES
WHERE FIRST_NAME LIKE 'S%' OR LAST_NAME LIKE 'S%';
<--11-->
SELECT *
FROM EMPLOYEES
WHERE HIRE_DATE LIKE '%MAY%';
<--12-->
SELECT *
FROM EMPLOYEES
WHERE COMMISSION_PCT IS NULL AND (SALARY BETWEEN 5000 AND 10000) AND (DEPARTMENT_ID = 30);
<--13-->
SELECT FIRST_NAME, HIRE_DATE+30
FROM EMPLOYEES;
<--14-->
SELECT FIRST_NAME, SYSDATE-HIRE_DATE
FROM EMPLOYEES;
<--15-->
SELECT FIRST_NAME
FROM EMPLOYEES
WHERE HIRE_DATE LIKE '%-%-01';
<--16-->
SELECT FIRST_NAME, LAST_NAME
FROM EMPLOYEES;
<--17--> DO AGAIN
SELECT JOB_TITLE
FROM JOBS;
<--20-->
SELECT *
FROM EMPLOYEES 
WHERE to_char(HIRE_DATE,'yy') = to_char(SYSDATE,'YY');
<--21-->
SELECT ROUND(SYSDATE - TO_DATE('01-JAN-11'),0) FROM DUAL;
<--23-->
SELECT MANAGER_ID, COUNT(*)
FROM EMPLOYEES
GROUP BY MANAGER_ID;
<--24-->
SELECT DISTINCT EMPLOYEE_ID, END_DATE
FROM JOB_HISTORY;
<--25-->
SELECT COUNT(*)
FROM EMPLOYEES 
WHERE TO_CHAR(HIRE_DATE,'DD') > 15
GROUP BY to_char(HIRE_DATE,'Month');
<--26-->
SELECT COUNTRY_ID, COUNT(CITY)
FROM LOCATIONS 
GROUP BY(COUNTRY_ID);
<--27-->
SELECT DEPARTMENT_ID, AVG(SALARY)
FROM EMPLOYEES
WHERE COMMISSION_PCT IS NOT NULL
GROUP BY DEPARTMENT_ID;
<--28-->
SELECT JOB_ID, COUNT(EMPLOYEE_ID), SUM(SALARY), MAX(SALARY)-MIN(SALARY)
FROM EMPLOYEES
GROUP BY JOB_ID;
<--29-->
SELECT JOB_ID
FROM EMPLOYEES 
GROUP BY JOB_ID
HAVING AVG(SALARY)>10000;
<--30-->
SELECT COUNT(EMPLOYEE_ID), TO_CHAR(HIRE_DATE, 'YYYY')
FROM EMPLOYEES
GROUP BY TO_CHAR(HIRE_DATE, 'YYYY')
HAVING COUNT(EMPLOYEE_ID)>10;
<--31-->
SELECT DISTINCT E1.DEPARTMENT_ID
FROM EMPLOYEES  E1
WHERE E1.DEPARTMENT_ID IN(SELECT E2.DEPARTMENT_ID
                            FROM EMPLOYEES E2
                            WHERE COMMISSION_PCT IS NOT NULL
                            GROUP BY E2.DEPARTMENT_ID 
                            HAVING COUNT(*)>5);
<--32-->
SELECT EMPLOYEE_ID
FROM JOB_HISTORY
GROUP BY EMPLOYEE_ID
HAVING COUNT(*)>1;
<--33-->
SELECT JOB_ID
FROM EMPLOYEES
GROUP BY JOB_ID
HAVING COUNT(*)>3;
<--34-->
SELECT DEPARTMENT_ID, TO_CHAR(SYSDATE, 'YYYY'), COUNT(EMPLOYEE_ID)
FROM EMPLOYEES
GROUP BY DEPARTMENT_ID;
<--35-->
SELECT DISTINCT E1.DEPARTMENT_ID
FROM EMPLOYEES E1
WHERE E1.MANAGER_ID IN (SELECT E2.MANAGER_ID
                            FROM EMPLOYEES E2
                            GROUP BY MANAGER_ID
                            HAVING COUNT(EMPLOYEE_ID)>5);
<--40-->
SELECT d.DEPARTMENT_NAME, COUNT(*)
FROM EMPLOYEES e, DEPARTMENTS d
WHERE e.DEPARTMENT_ID = d.DEPARTMENT_ID
GROUP BY d.DEPARTMENT_NAME;
<--41-->
SELECT j2.JOB_TITLE, j1.EMPLOYEE_ID, ROUND(j1.END_DATE - j1.START_DATE,0), j1.START_DATE
FROM JOB_HISTORY j1, JOBS j2
WHERE j1.JOB_ID = j2.JOB_ID;
<--42-->
SELECT D.DEPARTMENT_NAME, E.FIRST_NAME
FROM DEPARTMENTS D, EMPLOYEES E
WHERE D.MANAGER_ID = E.EMPLOYEE_ID;
<--43-->
SELECT D.DEPARTMENT_NAME, E.FIRST_NAME, E.LAST_NAME, L.CITY
FROM DEPARTMENTS D, EMPLOYEES E, LOCATIONS L
WHERE D.MANAGER_ID = E.EMPLOYEE_ID AND D.LOCATION_ID = L.LOCATION_ID;
<--44-->
SELECT L.COUNTRY_ID, L.CITY, D.DEPARTMENT_NAME
FROM DEPARTMENTS D, LOCATIONS L
WHERE D.LOCATION_ID = L.LOCATION_ID;
<--45-->
SELECT J.JOB_TITLE, d.DEPARTMENT_NAME, E.LAST_NAME, H.START_DATE
FROM JOBS J, DEPARTMENTS D, JOB_HISTORY H, EMPLOYEES E
WHERE H.JOB_ID = J.JOB_ID AND 
        H.EMPLOYEE_ID = E.EMPLOYEE_ID AND
        H.DEPARTMENT_ID = D.DEPARTMENT_ID AND
        TO_CHAR(H.START_DATE,'YYYY') BETWEEN 2000 AND 2005;
<--46-->
SELECT J.JOB_TITLE, AVG(E.SALARY)
FROM EMPLOYEES E, JOBS J
WHERE E.JOB_ID = J.JOB_ID
GROUP BY J.JOB_TITLE;
<--47-->
SELECT J.JOB_TITLE, E.FIRST_NAME, E.LAST_NAME, (E.SALARY-J.MAX_SALARY)
FROM EMPLOYEES E, JOBS J
WHERE E.JOB_ID = J.JOB_ID;
<--48-->
SELECT E.LAST_NAME, J.JOB_TITLE
FROM EMPLOYEES E, JOBS J
WHERE E.JOB_ID = J.JOB_ID AND
        E.COMMISSION_PCT IS NOT NULL
        AND E.DEPARTMENT_ID = 30;
<--49-->
SELECT J.*
FROM EMPLOYEES E, JOBS J
WHERE E.JOB_ID = J.JOB_ID AND E.SALARY > 15000;
<--50-->
SELECT D.DEPARTMENT_NAME, E.FIRST_NAME, E.LAST_NAME, E.SALARY
FROM DEPARTMENTS D, EMPLOYEES E
WHERE D.MANAGER_ID = E.EMPLOYEE_ID;
<--51-->
SELECT E1.FIRST_NAME, E1.LAST_NAME
FROM EMPLOYEES E1, EMPLOYEES E2
WHERE E1.MANAGER_ID = E2.EMPLOYEE_ID AND
    E2.HIRE_DATE > E1.HIRE_DATE;
<--52-->
SELECT E.FIRST_NAME, E.LAST_NAME, J.JOB_TITLE
FROM JOB_HISTORY H, EMPLOYEES E, JOBS J
WHERE H.EMPLOYEE_ID = E.EMPLOYEE_ID AND
        H.JOB_ID = J.JOB_ID AND
        H.END_DATE-H.START_DATE < 180;
<--53-->
SELECT E.FIRST_NAME, E.LAST_NAME, L.COUNTRY_ID, L.CITY
FROM EMPLOYEES E, DEPARTMENTS D, LOCATIONS L
WHERE E.DEPARTMENT_ID = D.DEPARTMENT_ID AND D.LOCATION_ID = L.LOCATION_ID;
<--54-->
SELECT D.DEPARTMENT_NAME, ROUND(AVG(E.SALARY),2), COUNT(E.EMPLOYEE_ID)
FROM EMPLOYEES E, DEPARTMENTS D
WHERE E.DEPARTMENT_ID = D.DEPARTMENT_ID
AND E.COMMISSION_PCT IS NOT NULL
GROUP BY D.DEPARTMENT_NAME;
<--55-->
WITH SUBQ AS (
    SELECT D.DEPARTMENT_ID AS DEPTS
    FROM DEPARTMENTS D, LOCATIONS L
    WHERE D.LOCATION_ID = L.LOCATION_ID
    AND L.CITY = 'Sydney'
    )
SELECT TO_CHAR(E.HIRE_DATE, 'MM'), COUNT(*)
FROM EMPLOYEES E, SUBQ S
WHERE E.DEPARTMENT_ID = S.DEPTS
GROUP BY TO_CHAR(E.HIRE_DATE, 'MM')
HAVING COUNT(*)>5;
<--56-->
WITH SUBQ AS  (
    SELECT DEPARTMENT_ID, MAX(SALARY)
    FROM EMPLOYEES
    GROUP BY DEPARTMENT_ID
    HAVING MAX(SALARY)>10000
)
SELECT D.*
FROM DEPARTMENTS D, SUBQ S
WHERE D.DEPARTMENT_ID = S.DEPARTMENT_ID;
<--57-->
SELECT D.*, E.FIRST_NAME, E.LAST_NAME
FROM DEPARTMENTS D, EMPLOYEES E
WHERE D.MANAGER_ID = E.EMPLOYEE_ID
AND E.LAST_NAME = 'Smith';
<--59-->
